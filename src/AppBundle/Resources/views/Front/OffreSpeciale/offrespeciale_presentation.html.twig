<div class="boat-presentation">
    <div class="col-md-9">
        {{ offreSpeciale.description|raw }}
        <a href="{{ path('skipper', { 'id': skipper.id }) }}#contact" class="btn btn-warning">{{'contact_skipper'|trans}}</a> 
    </div>
    <div class="col-md-3 panel-offrespeciale-prestation panel panel-info">
		<div class="panel-heading">
			<h3 class="panel-title">{{'boat_prestation_available'|trans }}</h3>
		</div>
    	<div class="panel-body">
	    	{% for prestation in prestations %}
	    		<div class="prestation">
	    			<img src="{{ prestation.iconeWebPath|replace({ ('75'): '25' }) }}" class="img-responsive img-thumbnails pull-left" />
	    			{{ prestation.name }}
	    		</div>
	    	{% endfor %}
    	</div>
    </div>
</div>
