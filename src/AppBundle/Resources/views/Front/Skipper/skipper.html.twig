{% extends "AppBundle:Front:layout.html.twig" %}
{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('bundles/app/js/lib/slick.js') }}" type="text/javascript"></script>  
    <script src="{{ asset('bundles/app/js/skipper/skipper_available.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/app/js/boat/boat_contact.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/app/js/skipper/skipper_desti.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/app/js/skipper/skipper_price.js') }}" type="text/javascript"></script>  
{% endblock %}
{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" media="all" href="{{ asset('bundles/app/css/lib/slick.css') }}">
    <link rel="stylesheet" media="all" href="{{ asset('bundles/app/css/lib/slick-theme.css') }}">
    <link rel="stylesheet" media="all" href="{{ asset('bundles/app/css/skippers.css') }}">
{% endblock %}   
{% block title %}{{'toptitle'|trans}} - {{'crew'|trans}}{% endblock %}
{% block content %}
    {%  render(controller('AppBundle:Skipper:submenu', { 'route': app.request.get('_route'), 'id': app.request.get('id') })) %}
    
    {% include "AppBundle:Front:Skipper/skipper_presentation.html.twig" %}
    {% include "AppBundle:Front:Skipper/skipper_bateau.html.twig" %}
    {% include "AppBundle:Front:Skipper/skipper_desti.html.twig" %}
    {% include "AppBundle:Front:Skipper/skipper_available.html.twig" %}
    {% include "AppBundle:Front:Skipper/skipper_price.html.twig" %}
    {% include "AppBundle:Front:Skipper/skipper_contact.html.twig" %}
    
{% endblock content %}